@import '../../styl/component-include.styl'

.b-breadcrumbs
  line-height 68px
  font-size 11pt
  overflow hidden
  text-overflow ellipsis
  white-space nowrap
  
  a
    color #9c9e9f
    
  span
    color #000
    text-transform uppercase
    
  a span
    color #9c9e9f
    text-transform none
    
.b-price-list
  line-height 68px
  font-size 11pt
  
  @media ( max-width 767px )
    text-align left
    line-height 28px